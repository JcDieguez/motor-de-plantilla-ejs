<br><br>
<div>
    <h2 style="color: brown;">Historial</h2>
    <% if (productos.length > 0) { %>
     <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th scope="col">Nombre</th>
          <th scope="col">Precio</th>
          <th scope="col">Imagen</th>
        </tr>
      </thead>
      <tbody>
        <% for(producto of productos) { %>
          <tr>
            <th><%= producto.name %></th>
            <th><%= producto.precio %></th>
            <th><%= producto.imagen %></th>
            <td>
              <img src=<%= producto.imagen %>  alt="img" height="100px">
          </td>
          </tr>
        <% } %>
      </tbody>
     </table>
    <% } else {%>
        <p class="alert alert-danger">No se encontr√≥ data, lo sentimos!</p>
    <% }%>
</div>